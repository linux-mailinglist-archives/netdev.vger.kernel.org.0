-			const struct sh_pfc_pin *pin2 = &info->pins[j];
-
-			if (same_name(pin->name, pin2->name))
-				sh_pfc_err("pin %s: name conflict\n",
-					   pin->name);
-
-			if (pin->pin != (u16)-1 && pin->pin == pin2->pin)
