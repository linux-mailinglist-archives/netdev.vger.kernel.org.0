-					   pin->name, pin2->name, pin->pin);
-
-			if (pin->enum_id && pin->enum_id == pin2->enum_id)
-				sh_pfc_err("pin %s/%s: enum_id %u conflict\n",
-					   pin->name, pin2->name,
-					   pin->enum_id);
-		}
-
-		if (pin->configs & SH_PFC_PIN_CFG_PULL_UP_DOWN) {
