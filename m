+static inline int
+entrypoints_bpf__hid_rdesc_fixup__attach(struct entrypoints_bpf *skel)
+{
+	int prog_fd = skel->progs.hid_rdesc_fixup.prog_fd;
+	int fd = skel_raw_tracepoint_open(NULL, prog_fd);
+
+	if (fd > 0)
+		skel->links.hid_rdesc_fixup_fd = fd;
+	return fd;
+}
+
